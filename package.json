{
  "name": "primeng-webpack2-starter",
  "version": "v1.1.0",
  "description": "Angular2 + PrimeNG + Webpack2 Starter with AoT support.",
  "scripts": {
    "ngc": "ngc -p ./tsconfig-aot.json",
    "webpack": "webpack --config=webpack-aot.config.js -p --progress",
    "start": "webpack-dev-server --progress --watch --port 3300",
    "build": "npm run ngc && npm run webpack",
    "server": "live-server --entry-file=index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ooboqoo/primeng-webpack2-starter"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "chart.js": "^2.4.0",
    "core-js": "^2.4.1",
    "primeng": "^1.1.2",
    "rxjs": "5.0.2",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.1",
    "@angular/platform-server": "2.4.1",
    "@types/core-js": "^0.9.35",
    "@types/node": "^6.0.54",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "3.0.0-beta.17",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "live-server": "^1.1.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.9",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "typescript": "2.0.10",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.12"
  }
}
